{
  "name": "gatsby-starter-bootstrap",
  "description": "Simple starter for Gatsby",
  "version": "3.0.0",
  "author": "Futoshi Iwashita <jaxx2104@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "animate.css": "4.1.1",
    "bootstrap": "~5.1",
    "emergence.js": "1.1.2",
    "font-awesome": "4.7.0",
    "gatsby": "~3.14",
    "gatsby-image": "~3.11",
    "gatsby-link": "~3.14",
    "gatsby-plugin-catch-links": "~3.14",
    "gatsby-plugin-manifest": "~3.14",
    "gatsby-plugin-netlify": "~3.14",
    "gatsby-plugin-offline": "~4.12",
    "gatsby-plugin-react-helmet": "~4.14",
    "gatsby-plugin-sass": "~4.14",
    "gatsby-plugin-sharp": "~3.14",
    "gatsby-plugin-sitemap": "~3.3",
    "gatsby-plugin-twitter": "~3.14",
    "gatsby-remark-copy-linked-files": "~3.2",
    "gatsby-remark-images": "~4.2",
    "gatsby-remark-prismjs": "~4.2",
    "gatsby-remark-responsive-iframe": "~3.2",
    "gatsby-remark-smartypants": "~3.2",
    "gatsby-source-filesystem": "~3.14",
    "gatsby-transformer-remark": "~3.2",
    "gatsby-transformer-sharp": "~3.14",
    "jquery": "3.6.0",
    "modern-normalize": "~1.1",
    "popper.js": "~1.16",
    "prismjs": "~1.27",
    "react": "~17.0",
    "react-dom": "~17.0",
    "react-helmet": "~6.1",
    "sass": "~1.50"
  },
  "devDependencies": {
    "@types/react-helmet": "~6.1",
    "@typescript-eslint/eslint-plugin": "~5.18",
    "@typescript-eslint/parser": "~5.18",
    "babel-eslint": "10.1.0",
    "eslint": "~8.12",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-prettier": "~4.0",
    "eslint-plugin-react": "~7.29",
    "eslint-plugin-react-hooks": "~4.4",
    "gh-pages": "~3.2",
    "husky": "~7.0",
    "lint-staged": "~12.3",
    "prettier": "~2.6",
    "textlint": "~12.1",
    "textlint-rule-preset-ja-spacing": "~2.2",
    "textlint-rule-preset-japanese": "~7.0",
    "typescript": "latest"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "npm run lint:textfix"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\" --ignore-path .gitignore",
    "lint": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
