{
  "name": "gatsby-starter-bootstrap",
  "description": "Simple starter for Gatsby",
  "version": "3.0.0",
  "author": "Futoshi Iwashita <jaxx2104@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-brands-svg-icons": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "animate.css": "4.1.1",
    "bootstrap": "~5.3",
    "emergence.js": "1.1.2",
    "font-awesome": "4.7.0",
    "gatsby": "~5.13",
    "gatsby-link": "~5.13",
    "gatsby-plugin-catch-links": "~5.13",
    "gatsby-plugin-image": "~3.13",
    "gatsby-plugin-manifest": "~5.13",
    "gatsby-plugin-netlify": "~5.1",
    "gatsby-plugin-offline": "~6.13",
    "gatsby-plugin-react-helmet": "~6.13",
    "gatsby-plugin-sass": "~6.13",
    "gatsby-plugin-sharp": "~5.13",
    "gatsby-plugin-sitemap": "~6.13",
    "gatsby-plugin-twitter": "~5.13",
    "gatsby-remark-copy-linked-files": "~6.13",
    "gatsby-remark-images": "~7.13",
    "gatsby-remark-prismjs": "~7.13",
    "gatsby-remark-responsive-iframe": "~6.13",
    "gatsby-remark-smartypants": "~6.13",
    "gatsby-source-filesystem": "~5.13",
    "gatsby-transformer-remark": "~6.13",
    "gatsby-transformer-sharp": "~5.13",
    "jquery": "~3.7",
    "modern-normalize": "~2.0",
    "popper.js": "~1.16",
    "prismjs": "~1.29",
    "react": "~18.3",
    "react-dom": "~18.3",
    "react-helmet": "~6.1",
    "sass": "~1.76"
  },
  "devDependencies": {
    "@types/react-helmet": "~6.1",
    "@typescript-eslint/eslint-plugin": "4.0.0",
    "@typescript-eslint/parser": "7.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "~9.1",
    "eslint-plugin-prettier": "~5.1",
    "eslint-plugin-react": "~7.34",
    "eslint-plugin-react-hooks": "~4.6",
    "gh-pages": "~6.1",
    "husky": "~9.0",
    "lint-staged": "~15.2",
    "prettier": "~3.2",
    "textlint": "~14.0",
    "textlint-rule-preset-ja-spacing": "~2.4",
    "textlint-rule-preset-japanese": "~10.0",
    "typescript": "5.4"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "npm run lint:textfix"
    ]
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\" --ignore-path .gitignore",
    "lint": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
